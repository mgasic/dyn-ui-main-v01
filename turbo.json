{
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "@dyn-ui/design-tokens#build"],
      "outputs": ["dist/**", ".next/**", "styles/generated/**"],
      "cache": true
    },
    "@dyn-ui/design-tokens#build": {
      "outputs": ["styles/generated/**", "build/**", "dist/**"],
      "cache": true,
      "inputs": ["tokens/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["^test"],
      "outputs": []
    }
  }
}